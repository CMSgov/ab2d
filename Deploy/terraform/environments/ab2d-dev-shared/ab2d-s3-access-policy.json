{
    "Version": "2012-10-17",
    "Statement": [
	{
            "Effect": "Allow",
            "Action": [
                "s3:Get*",
                "s3:List*"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": [
		"arn:aws:s3:::ab2d-dev-automation/*",
		"arn:aws:s3:::ab2d-dev-cloudtrail/*",
		"arn:aws:s3:::ab2d-dev/*",
		"arn:aws:s3:::ab2d-optout-data-dev/*"
            ]
        }
    ]
}
